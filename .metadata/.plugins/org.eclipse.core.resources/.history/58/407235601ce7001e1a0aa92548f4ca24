package br.com.senaisp.bauru.giulia.licao04secap01;

public class FestaAlex {
	//Constantes
	final static double IMPOSTO = 0.05;
	final static double GORJETA = 0.15;
	
	public static double calcularConta(double valor) {
	return valor *	( 1 + IMPOSTO ) * ( 1 + GORJETA );
	}
	public static void main(String[] args) {
		double pessoa1 = 10;
		double pessoa2 = 12;
		double pessoa3 = 9;
		double pessoa4 = 8;
		double pessoa5 = 7;
		double pessoa6 = 15;
		double pessoa7 = 11;
		double pessoa8 = 30;
		
		
		double total = calcularConta(pessoa1);
		double tota2 = calcularConta(pessoa2);
		double tota3 = calcularConta(pessoa3);
		double tota4 = calcularConta(pessoa4);
		double tota5 = calcularConta(pessoa5);
		double tota6 = calcularConta(pessoa6);
		double tota7 = calcularConta(pessoa7);
		double tota8 = calcularConta(pessoa8);
	}

}
